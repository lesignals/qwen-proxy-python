# Server configuration
PORT=8080
HOST=localhost

# API Authentication configuration
# Set this API Key to protect proxy server access
# If not set, authentication will be disabled
API_KEY=your-api-key-here

# Debug logging configuration
# Set to true to enable debug logging (log files will be created)
DEBUG_LOG=false

# Maximum number of debug log files to keep (older files will be deleted)
LOG_FILE_LIMIT=20

# Streaming configuration
# Set to 'true' to enable streaming responses for dify/chatbox compatibility (default is false)
STREAM=true